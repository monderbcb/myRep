<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="UniversityPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>university.Entites.Subject</class>
    <class>university.Entites.Instructor</class>
    <class>university.Entites.Users</class>
    <class>university.Entites.StudentCourse</class>
    <class>university.Entites.SubDepartment</class>
    <class>university.Entites.Course</class>
    <class>university.Entites.Department</class>
    <class>university.Entites.Section</class>
    <class>university.Entites.Student</class>
    <class>university.Entites.Semester</class>
    <class>university.Entites.Semester_1</class>
    <class>university.Entites.Section_1</class>
    <class>university.Entites.Users_1</class>
    <class>university.Entites.StudentCourse_1</class>
    <class>university.Entites.SubDepartment_1</class>
    <class>university.Entites.Subject_1</class>
    <class>university.Entites.Instructor_1</class>
    <class>university.Entites.Course_1</class>
    <class>university.Entites.Student_1</class>
    <class>university.Entites.Department_1</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/university?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
